{
  "name": "catchify",
  "version": "1.0.3",
  "description": "A collection of front end files to kickstart projects",
  "main": "js/index.js",
  "scripts": {
    "format": "node scripts/format.js ./node_modules './js/**/*.js'",
    "build:js": "node scripts/js.js ./node_modules ./js/index.js ./build/bundle.js",
    "build:css": "node scripts/css.js ./node_modules ./sass/styles.scss ./build ./build/styles.css",
    "build": "npm run build:css && npm run format && npm run build:js",
    "test:js": "JASMINE_CONFIG_PATH=js/tests/jasmine.json node ./node_modules/.bin/babel-node --presets node6 ./node_modules/.bin/jasmine",
    "test": "npm run build && npm run test:js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/catchdigital/catchify.git"
  },
  "author": "Stuart Wilson <s.wilson@catchdigital.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.7.3",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "bootstrap-sass": "^3.3.6",
    "browserify": "^14.1.0",
    "log-timestamp": "^0.1.2",
    "node-sass": "^4.5.0",
    "node.normalize.scss": "^3.0.3",
    "postcss-cli": "^3.0.0-beta",
    "prettier": "^1.8.2",
    "babel-preset-stage-3": "^6.24.1"
  },
  "engines": {
    "node": "~6.0.0"
  },
  "devDependencies": {
    "babel-preset-node6": "^11.0.0",
    "jasmine": "^2.5.3",
    "jsdom": "^9.12.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-3"
          ]
        }
      ]
    ]
  }
}
